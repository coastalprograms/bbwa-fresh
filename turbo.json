{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env",
    "**/.env.local",
    "**/.env.production",
    "**/tailwind.config.*",
    "**/next.config.*",
    "**/supabase/config.toml"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "VERCEL",
    "NETLIFY",
    "CI"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY",
        "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
        "AI_PROVIDER",
        "FAQ_SOURCE"
      ]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/.next/**",
        "apps/web/out/**",
        "apps/web/.turbo/**"
      ],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY", 
        "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY",
        "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
        "SUPABASE_SERVICE_ROLE_KEY",
        "AI_PROVIDER",
        "GEMINI_API_KEY",
        "OPENAI_API_KEY",
        "FAQ_SOURCE",
        "FAQ_LIMIT",
        "SESSION_SECRET",
        "AIRTABLE_API_KEY",
        "AIRTABLE_BASE_ID",
        "AIRTABLE_TABLE",
        "AIRTABLE_VIEW",
        "AIRTABLE_FIELDS_QUESTION",
        "AIRTABLE_FIELDS_ANSWER",
        "AIRTABLE_FIELDS_UPDATED"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": true
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": [],
      "cache": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/.tsbuildinfo",
        "apps/web/tsconfig.tsbuildinfo"
      ],
      "cache": true
    },
    "tsc": {
      "dependsOn": ["typecheck"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/coverage/**"
      ],
      "cache": true,
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true,
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_SUPABASE_URL", 
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/coverage/**"
      ],
      "cache": true,
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ]
    },
    "env:validate": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "health": {
      "cache": false,
      "outputs": []
    }
  },
  "remoteCache": {
    "signature": true
  },
  "ui": "tui"
}